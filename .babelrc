{
  "env": {
    "development": {
      "presets": [
				"@vue/babel-preset-jsx",
        ["@babel/preset-env", { "modules": false }]
      ]
    },
    "test": {
      "presets": [
        "@vue/babel-preset-jsx",
        ["@babel/preset-env", {
          "modules": false,
          "targets": { "node": "current" }
        }]
      ],
      "plugins": ["istanbul"]
    }
  }
}